{
  "name": "@dhruv-m-patel/web-app",
  "version": "1.2.0",
  "engines": {
    "node": ">= 14.15.5",
    "npm": ">= 6.14.11"
  },
  "main": "build/index.js",
  "repository": {
    "type": "git",
