    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^28.1.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.14.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
